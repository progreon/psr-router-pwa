{
  "name": "psr-router-pwa",
  "version": "0.2.0",
  "description": "A web application for routing Pokemon SpeedRuns",
  "private": true,
  "scripts": {
    "serve": "yarn serve:dev",
    "serve:dev": "APP_VERSION=$npm_package_version webpack serve --config config/webpack/dev.config.js",
    "serve:prod": "APP_VERSION=$npm_package_version webpack serve --config config/webpack/prod.config.js",
    "start": "yarn start:prod",
    "start:dev": "yarn build:dev && http-server dist -p 5000 -c-1",
    "start:prod": "yarn build:prod && http-server dist -p 5000 -c-1",
    "build": "yarn build:prod",
    "build:dev": "APP_VERSION=$npm_package_version webpack --config config/webpack/dev.config.js",
    "build:prod": "APP_VERSION=$npm_package_version webpack --config config/webpack/prod.config.js",
    "build:profile": "yarn build:profile:prod",
    "build:profile:dev": "webpack --config config/webpack/dev.config.js --profile --json > stats.json",
    "build:profile:prod": "webpack --config config/webpack/prod.config.js --profile --json > stats.json",
    "genjsdoc": "jsdoc -r -d jsdoc ./src",
    "test": "echo \"Error: no test specified\" && exit 1",
    "ver": "echo v$npm_package_version",
    "watch": "webpack --watch --config config/webpack/dev.config.js",
    "server": "node server.js"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:progreon/psr-router-pwa.git"
  },
  "keywords": [
    "psr",
    "router",
    "pokemons",
    "speedrun",
    "speedruns",
    "speed",
    "run",
    "runs"
  ],
  "author": "Marco Willems",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/progreon/psr-router-pwa/issues"
  },
  "homepage": "https://github.com/progreon/psr-router-pwa#readme",
  "devDependencies": {
    "@babel/core": "^7.12.7",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.12.7",
    "@types/file-saver": "^2.0.0",
    "babel-loader": "^8.2.2",
    "css-loader": "^6.2.0",
    "express": "^4.16.3",
    "file-loader": "^6.2.0",
    "fork-ts-checker-webpack-plugin": "^6.2.12",
    "handlebars": "^4.0.11",
    "html-webpack-plugin": "^5.3.2",
    "http-server": "^0.11.1",
    "jsdoc": "^3.5.5",
    "raw-loader": "^4.0.2",
    "style-loader": "^3.2.1",
    "ts-loader": "^9.2.3",
    "typescript": "^4.3.5",
    "webpack": "^5.45.1",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "^3.11.2",
    "webpack-manifest-plugin": "^3.1.1",
    "webpack-merge": "^5.8.0",
    "workbox-webpack-plugin": "^6.1.5",
    "xml-loader": "^1.2.1"
  },
  "dependencies": {
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@material/mwc-button": "^0.20.0",
    "@material/mwc-dialog": "^0.20.0",
    "@material/mwc-drawer": "^0.20.0",
    "@material/mwc-formfield": "^0.20.0",
    "@material/mwc-icon": "^0.20.0",
    "@material/mwc-select": "^0.20.0",
    "@material/mwc-switch": "^0.20.0",
    "@material/mwc-textarea": "^0.20.0",
    "@material/mwc-textfield": "^0.20.0",
    "@polymer/app-layout": "^3.0.1",
    "@polymer/paper-toast": "^3.0.1",
    "@polymer/polymer": "^3.0.2",
    "@vaadin/vaadin-text-field": "^2.8.2",
    "@webcomponents/webcomponentsjs": "^2.0.2",
    "file-saver": "^1.3.8",
    "harmony-reflect": "^1.6.1",
    "material-design-icons": "^3.0.1",
    "pwa-helpers": "^0.8.3",
    "redux": "^4.0.0",
    "redux-thunk": "^2.3.0",
    "whatwg-fetch": "^2.0.4"
  },
  "resolutions": {
    "lit-element": "2.3.0",
    "lit-html": "1.3.0"
  },
  "browserslist": [
    "last 2 versions",
    "not ie <= 11"
  ]
}
