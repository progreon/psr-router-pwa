{
  "name": "psr-router-pwa",
  "version": "0.2.0",
  "description": "A web application for routing Pokemon SpeedRuns",
  "private": true,
  "scripts": {
    "serve": "cross-env NODE_OPTIONS=--openssl-legacy-provider yarn serve:dev",
    "serve:dev": "cross-env APP_VERSION=$npm_package_version webpack-dev-server --config config/webpack/dev.config.js",
    "serve:prod": "cross-env APP_VERSION=$npm_package_version webpack-dev-server --config config/webpack/prod.config.js",
    "start": "yarn start:prod",
    "start:dev": "yarn build:dev && http-server dist -p 5000 -c-1",
    "start:prod": "yarn build:prod && http-server dist -p 5000 -c-1",
    "build": "cross-env NODE_OPTIONS=--openssl-legacy-provider yarn build:prod",
    "build:dev": "cross-env APP_VERSION=$npm_package_version webpack --config config/webpack/dev.config.js",
    "build:prod": "cross-env APP_VERSION=$npm_package_version webpack --config config/webpack/prod.config.js",
    "build:profile": "yarn build:profile:prod",
    "build:profile:dev": "webpack --config config/webpack/dev.config.js --profile --json > stats.json",
    "build:profile:prod": "webpack --config config/webpack/prod.config.js --profile --json > stats.json",
    "genjsdoc": "jsdoc -r -d jsdoc ./src",
    "test": "echo \"Error: no test specified\" && exit 1",
    "ver": "echo v$npm_package_version",
    "watch": "webpack --watch --config config/webpack/dev.config.js",
    "server": "node server.js"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:progreon/psr-router-pwa.git"
  },
  "keywords": [
    "psr",
    "router",
    "pokemons",
    "speedrun",
    "speedruns",
    "speed",
    "run",
    "runs"
  ],
  "author": "Marco Willems",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/progreon/psr-router-pwa/issues"
  },
  "homepage": "https://github.com/progreon/psr-router-pwa#readme",
  "devDependencies": {
    "@babel/core": "^7.12.7",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.12.7",
    "@types/file-saver": "^2.0.0",
    "babel-loader": "^8.0.5",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "clean-webpack-plugin": "^0.1.19",
    "copy-webpack-plugin": "^4.5.2",
    "cross-env": "^7.0.3",
    "css-loader": "^5.0.1",
    "csv-loader": "^3.0.2",
    "express": "^4.16.3",
    "favicons-webpack-plugin": "^0.0.9",
    "file-loader": "^6.2.0",
    "handlebars": "^4.0.11",
    "html-webpack-plugin": "^3.2.0",
    "http-server": "^0.11.1",
    "jsdoc": "^3.5.5",
    "mini-css-extract-plugin": "^1.3.1",
    "optimize-css-assets-webpack-plugin": "^4.0.3",
    "papaparse": "^4.5.0",
    "raw-loader": "^4.0.2",
    "style-loader": "^0.21.0",
    "svg-loader": "^0.0.2",
    "ts-loader": "^8.0.0",
    "typescript": "^4.1.2",
    "uglifyjs-webpack-plugin": "^1.2.7",
    "webpack": "webpack-4",
    "webpack-bundle-analyzer": "^2.13.1",
    "webpack-cli": "^3.0.8",
    "webpack-dev-middleware": "^3.1.3",
    "webpack-dev-server": "^3.1.4",
    "webpack-manifest-plugin": "^2.0.3",
    "webpack-merge": "^4.1.3",
    "webpack-pwa-manifest": "^3.7.1",
    "workbox-webpack-plugin": "^3.4.1",
    "xml-loader": "^1.2.1"
  },
  "dependencies": {
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@material/mwc-button": "^0.20.0",
    "@material/mwc-dialog": "^0.20.0",
    "@material/mwc-drawer": "^0.20.0",
    "@material/mwc-formfield": "^0.20.0",
    "@material/mwc-icon": "^0.20.0",
    "@material/mwc-select": "^0.20.0",
    "@material/mwc-switch": "^0.20.0",
    "@material/mwc-textarea": "^0.20.0",
    "@material/mwc-textfield": "^0.20.0",
    "@polymer/app-layout": "^3.0.1",
    "@polymer/paper-toast": "^3.0.1",
    "@polymer/polymer": "^3.0.2",
    "@vaadin/vaadin-text-field": "^2.8.2",
    "@webcomponents/webcomponentsjs": "^2.0.2",
    "es6-promise": "^4.2.4",
    "file-saver": "^1.3.8",
    "harmony-reflect": "^1.6.1",
    "lit-element": "^2.4.0",
    "material-design-icons": "^3.0.1",
    "pwa-helpers": "^0.8.3",
    "redux": "^4.0.0",
    "redux-thunk": "^2.3.0",
    "url-polyfill": "^1.0.13",
    "whatwg-fetch": "^2.0.4"
  },
  "browserslist": [
    "last 2 versions",
    "not ie <= 10",
    "not ie_mob <= 10"
  ]
}
